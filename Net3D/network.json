{
    "params":
    {
        "learning_rate" : 0.001,
        "num_epochs" : 20,
        "l2_reg_weights" : 0.3,
        "l2_reg_biases" : 0.1,
        "decay_step" : 10000,
        "decay_rate" : 0.96,
        "min_prob" : 0.65,
        "keep_prob" : 0.65,
        "input_shape" : [32,32,32,1],
        "batch_size" : 32,
        "num_classes" : 11
    },

    "layers":
    [
        {
            "name" : "conv0",
            "filter" : [5,5,5,1,64],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },

        {
            "name" : "conv1",
            "filter" : [5,5,5,64,64],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },
        
        {
            "name" : "conv2",
            "filter" : [5,5,5,64,128],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },

        {
            "name" : "conv3",
            "filter" : [5,5,5,128,128],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },

        {
            "name" : "conv4",
            "filter" : [5,5,5,128,256],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },

        {
            "name" : "conv5",
            "filter" : [5,5,5,256,256],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },

        {
            "name" : "conv6",
            "filter" : [5,5,5,256,512],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid",
            "dropout" : "yes"
        },

        {
            "name" : "conv7",
            "filter" : [4,4,4,512,512],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid"
        },

        {
            "name" : "conv8",
            "filter" : [1,1,1,512,256],
            "stride" : [1,1,1,1,1],
            "activation" : "relu",
            "padding" : "valid"
        },

        {
            "name" : "conv9",
            "filter" : [1,1,1,256,11],
            "stride" : [1,1,1,1,1],
            "padding" : "valid"
        }
    ]

}